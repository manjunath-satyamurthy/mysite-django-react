{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-loader": "^1.0.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.5.3",
    "promise": "7.1.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-transition-group": "^1.2.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node app/react/scripts/start.js",
    "build": "node app/react/scripts/build.js",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.0.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.18.2",
    "uglify-js": "^2.8.29",
    "webpack": "^3.4.1",
    "webpack-bundle-tracker": "^0.2.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/app/react/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/app/react/tests/*.js"
    ],
    "testURL": "http://localhost",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/app/react/config/jestMocks/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/app/react/config/jestMocks/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "test": {
        "presets": ["es2015", "react"]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
